{
  "manifest_version": 3,
  "name": "EduTube",
  "version": "1.0.0",
  "description": "EduTube learning journeys - sign in and manage your journeys from the browser.",
  "action": {},
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["sidePanel", "storage"],
  "host_permissions": [
    "http://localhost:5173/*",
    "http://localhost:5000/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
